unit PDVUpdates.Model.Usuario.Tipo.Caixa;

interface

uses PDVUpdates.Model.Usuario.Interfaces;

type

  TModelUsuarioTipoCAixa = class(TInterfacedObject, iModelUsuarioMetodos)
